{
  "expo": {
    "name": "SnapEvent",
    "slug": "snapevent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snapevent.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SnapEvent needs your location to help you connect with friends and attend events nearby."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.snapevent.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-router"
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "SnapEvent needs your location to help you connect with friends and attend events nearby."
        }
      ]
    ],
    "scheme": "snapevent",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90022c1c-fff9-41cc-83bc-d0fe6132aea9"
      }
    },
    "owner": "stealthhydrac"
  }
}
